<p id="notice"><%= notice %></p>

<h1>Battings</h1>

<table>
  <thead>
    <tr>
      <th>Playerid</th>
      <th>Yearid</th>
      <th>League</th>
      <th>Teamid</th>
      <th>Gamesplayed</th>
      <th>Hits</th>
      <th>Doubles</th>
      <th>Triples</th>
      <th>Homeruns</th>
      <th>Runsbattedin</th>
      <th>Stolenbases</th>
      <th>Caughtstealing</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @battings.each do |batting| %>
      <tr>
        <td><%= batting.playerID %></td>
        <td><%= batting.yearID %></td>
        <td><%= batting.league %></td>
        <td><%= batting.teamID %></td>
        <td><%= batting.gamesPlayed %></td>
        <td><%= batting.hits %></td>
        <td><%= batting.doubles %></td>
        <td><%= batting.triples %></td>
        <td><%= batting.homeruns %></td>
        <td><%= batting.runsBattedIn %></td>
        <td><%= batting.stolenBases %></td>
        <td><%= batting.caughtStealing %></td>
        <td><%= link_to 'Show', batting %></td>

        <td><%= link_to 'Destroy', batting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Batting', new_player_batting_path %>
